<Window x:Class="HttpClientAndRestSHarp_Test_Instance.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HttpClientAndRestSHarp_Test_Instance"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <WrapPanel Grid.Row="0">
            <TextBlock x:Name="WebData_User" Text="WebData_User" MouseDown="GetIntoUserTest" Margin="0 5 10 0" Background="Beige"  />
            <TextBlock x:Name="WebData_SmartDection"  Text="WebData_SmartDection" MouseDown="GetIntoSmartDectionTest" Margin="10 5 0 0" Background="AliceBlue" />
        </WrapPanel>
        <Grid x:Name="itemPane" Grid.Row="1"/>
    </Grid>
</Window>
